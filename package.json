{
  "name": "mobadraft",
  "version": "1.0.0",
  "description": "A web application dedicated to the drafting element of Moba PC games such as Heroes of The Storm, Dota 2, and League of Legends. This Will be used to improve the drafting element of players, amateur teams, and professional players.",
  "main": "index.js",
  "engines": {
    "node": "8.1.2",
    "npm": "5.4.2"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Nicholas March",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^3.6.1",
    "express": "^4.16.3",
    "jsonfile": "^4.0.0",
    "moment": "^2.22.2",
    "mongoose": "^4.13.14",
    "nodemon": "^1.18.3",
    "randomstring": "^1.1.5",
    "request": "^2.87.0",
    "socket.io": "^2.1.1",
    "winston": "^2.4.3"
  }
}
